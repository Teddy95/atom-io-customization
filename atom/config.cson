"*":
  "api-docs":
    _theme: "Dark"
  "atom-beautify":
    css:
      convert_quotes: "double"
      default_beautifier: "Pretty Diff"
      end_with_newline: true
      indent_size: 4
      no_lead_zero: true
      preserve_newlines: true
    general:
      beautifyEntireFileOnSave: false
    html:
      end_with_newline: true
      indent_inner_html: true
      indent_size: 4
      wrap_attributes_indent_size: 4
    js:
      end_with_comma: true
      end_with_newline: true
      indent_size: 4
      keep_array_indentation: true
      keep_function_indentation: true
      object_curly_spacing: true
      space_after_anon_function: true
    json:
      end_with_comma: true
      end_with_newline: true
      indent_size: 4
      keep_array_indentation: true
      keep_function_indentation: true
      object_curly_spacing: true
      space_after_anon_function: true
    markdown: {}
    scss:
      convert_quotes: "double"
      indent_size: 4
      preserve_newlines: true
    sql:
      identifiers: "lower"
      indent_size: 2
    twig:
      indent_size: 4
      space_after_anon_function: true
    xml:
      default_beautifier: "JS Beautify"
      end_with_newline: true
      indent_inner_html: true
      indent_size: 4
  "atom-html-preview": {}
  "atom-ide-ui":
    "atom-ide-diagnostics-ui":
      showDiagnosticTraces: true
      showDirectoryColumn: true
    "debugger-exceptionBreakpointsCollapsed": false
    hyperclick:
      darwinTriggerKeys: "ctrlKey"
  "atom-package-deps":
    ignored: [
      "language-marko"
      "linter-sass-lint"
    ]
  "atom-toolbar-preferred": {}
  "atom-touch-bar": {}
  "autocomplete-plus":
    confirmCompletion: "tab"
    minimumWordLength: 2
  "browser-plus":
    currentFile: false
    homepage: "https://devdocs.io"
  "color-picker":
